    "ts-loader": "^4.3.0",
    "tsify": "^4.0.0",
    "typescript": "^2.8.3",
    "gts": "^0.6.0"
  },
  "dependencies": {
    "d3": "^5.4.0",
    "d3-flextree": "^2.1.1"
  }
}
